{
  "name": "gmp_webpack",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "npm run dev",
    "prod:build": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --mode=development -w",
    "dev:start": "nodemon --experimental-modules server-build/serverBundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  },
